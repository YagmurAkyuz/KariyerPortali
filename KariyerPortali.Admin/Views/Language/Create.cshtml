<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <a href="@Url.Action("Index","Language")" class="nav-link ">Tanımlamalar</a>
                <i class="fa fa-circle"></i>
            </li>
            <li>
                <span>Yeni Kayıt</span>
            </li>
        </ul>
        <div class="page-toolbar">
            <div id="dashboard-report-range" class="pull-right tooltips btn btn-sm" data-container="body" data-placement="bottom" data-original-title="Change dashboard date range">
                <i class="icon-calendar"></i>&nbsp;
                <span class="thin uppercase hidden-xs"></span>&nbsp;
                <i class="fa fa-angle-down"></i>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 ">
            <!-- BEGIN SAMPLE FORM PORTLET-->
            <div class="portlet light bordered">
                <div class="portlet-title">
                    <div class="caption block">
                        <i class="icon-settings block"></i>
                        <span class="caption-subject bold uppercase"> Yeni Dil Ekleme</span>
                    </div>

                </div>
                <div class="portlet-body form">
                    <form role="form">
                        <div class="form-body">


                            <div class="form-group">
                                <label>Dil Adı</label>
                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Dil Adı Giriniz">

                                </div>
                            </div>


                        </div>
                        <div class="form-actions">
                            <button type="submit" class="btn blue">Gönder</button>
                            <button type="button" class="btn default">İptal</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!-- END SAMPLE FORM PORTLET-->
    <!-- BEGIN SAMPLE FORM PORTLET-->


</body>
